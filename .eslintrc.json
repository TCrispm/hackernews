{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "no-shadow": "off",
    "no-unused-vars": "off",
    "no-console": 1,
    "react/jsx-pascal-case": "off",
    "linebreak-style": 0
  }
}
